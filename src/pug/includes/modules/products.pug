.products(id="products")
  .wrapper
    h2.title.section-title Основные продукты компании
    .inner
      ul.list
        li.item.item-red(data-modal="confirm" data-title="Скачать методическое пособие" data-bg="assets/img/6products/bg.jpg")
          h3.red-title Разобраться в стендах легко
        each product, i in productsList
          li.item
            p.category !{product.category}
            h3.item-title !{product.title}
            if(product.label)
              span.label(class=product.labelColor) !{product.label}
            p.code !{product.code}

            .img
              +picture({
                 classList: 'images',
                 src: `assets/img/6products/${i + 1}.jpg`,

              })
            p.text !{product.text}
            button.btn(
              data-modal="product"
              data-title=product.title
              data-text=product.text
              data-img=`assets/img/6products/big/${i + 1}.jpg`
              data-price=product.price
              data-list=product.list.join('@')

            ) Узнать цену
    .all
      p.plus + Показать все продукты в категории







